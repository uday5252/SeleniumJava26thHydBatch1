package com.abc.demo;

import java.time.LocalDate;
import java.time.format.DateTimeFormatter;
import java.util.ArrayList;
import java.util.Collections;
import java.util.Comparator;

class DateComparator implements Comparator<String[]>
{

	@Override
	public int compare(String[] a, String[] b) {
//		a = {01-01-2023 14:00 ERROR failed},
//		b = {26-12-2024 23:15 ERROR failed},
		
		DateTimeFormatter datePattern = 
				DateTimeFormatter.ofPattern("dd-MM-yyyy");
		
		String date1 = a[0];
		String date2 = b[0];
		
		LocalDate javaDate1 = LocalDate.parse(date1, datePattern);
//		01-01-2023
		LocalDate javaDate2 = LocalDate.parse(date2, datePattern);
//		26-12-2024
		
		return javaDate1.compareTo(javaDate2);
	}
	
}

public class Second {

	public static void main(String[] args) {
		
	String[][] info = { 
			{"01-01-2023", "14:00", "ERROR", "failed"}, 
			{"01-12-2023", "15:00", "INFO", "established"}, 
			{"26-12-2024", "23:15", "ERROR", "failed"},
			{"22-05-2021", "12:00", "ERROR", "failed"} 	
		};
	
	ArrayList<String[]> al = new ArrayList<String[]>();
	
	for(int i = 0; i < info.length; i++)
	{
		if(info[i][2].equals("ERROR"))
				{
					al.add(info[i]);
				}
	}
	
	System.out.println("Before Sorting: "+al);
	for(String[] i : al)
	{
		System.out.println(i[0] + " " + i[1] + " " + i[2] + i[3]);
	}
	

	Collections.sort(al, new DateComparator());
	
	System.out.println("After Sorting: "+al);
	
	for(String[] i : al)
	{
		System.out.println(i[0] + " " + i[1] + " " + i[2] + i[3]);
	}
	
	

//	[ 
//	 {01-01-2023 14:00 ERROR failed},
//	 {26-12-2024 23:15 ERROR failed},
//	 {22-05-2021 12:00 ERROR failed},
//	]
	
//	[ 
//	 {22-05-2021 12:00 ERROR failed},
//	 {01-01-2023 14:00 ERROR failed},
//	 {26-12-2024 23:15 ERROR failed},
//	]
	
	
//	[ 
//		{"01-01-2023", "14:00", "ERROR", "failed"}, 
//		{"26-12-2024", "23:15", "ERROR", "failed"},
//		{"22-05-2021", "12:00", "ERROR", "failed"} 	
//	]
	
	}

}
