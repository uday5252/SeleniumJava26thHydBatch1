ArrayList al = new ArrayList();
		
		String s = "Please123 please123learn learn learn coding!";
		
		Pattern p = 
				Pattern.compile("(\\w+)\\s+\\1\\b", Pattern.CASE_INSENSITIVE);

		Matcher m = p.matcher(s);
		while(m.find())
		{
			al.add(m.group(1));
		}

		System.out.println(al);


========

		        String s = "Sooo excited for this coool event yesss absoulutely";

		        // Pattern to detect 3 or more consecutive repeated letters
		        Pattern p = Pattern.compile("(\\w)\\1{3}");

		        // Split the sentence into words
		        String[] words = s.split("\\s");

		        ArrayList<String> al = new ArrayList<>();

		        for (String word : words) {
		            Matcher m = p.matcher(word);
		            if (!m.find()) { // Only add words that DO NOT have 3+ repeated letters
		                al.add(word);
		            }
		        }

		        System.out.println(al);
		    }
		}
